<h1 class="text-center"><%= @view[:label] %></h1>

<div class="row">
  <div class="col-md-8">
    <div class="card">
      <div class="card-heading border-bottom">
        <h4 class="card-title">
          <%= t :annual_summary %>
        </h4>
      </div>
      <div class="card-block">
        <%= render partial: 'shared/annual_view' %>
      </div>
    </div>
  </div>
  <div class="col-md-4">
    <div class="card">
      <div class="card-heading border-bottom">
        <h4 class="card-title">
          <%= t :resources %>
        </h4>
      </div>
      <div class="card-block">
        <%= render partial: 'view/accordion' %>
      </div>
    </div>
  </div>
</div>
