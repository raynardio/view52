<div class="quote">
  <i class="fa fa-quote-left"></i>
  <span><%= @quote %></span>
  <i class="fa fa-quote-right"></i>
</div>
<div class="row">
  <div class="col-md-8">
    <div class="card">
        <div class="card-heading border-bottom">
          <h4 class="card-title">
            <%= t :annual_summary %>
          </h4>
        </div>
        <div class="card-block">
          <%= render partial: 'shared/annual_view' %>
        </div>
      </div>
    <div class="card">
      <div class="card-heading border-bottom">
        <h4 class="card-title">
          <%= t :agenda %>
