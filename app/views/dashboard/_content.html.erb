  </div>
  <div class="col-md-4">
    <div class="card">
      <div class="card-heading border-bottom">
        <h4 class="card-title">
          <%= t :roles_and_goals %>
        </h4>
      </div>
      <div class="card-block">
        <%= render partial: 'shared/roles_and_goals' %>
      </div>
    </div>
    <div class="card">
      <div class="card-heading border-bottom">
        <h4 class="card-title">
          <%= t :your_time %>
        </h4>
      </div>
      <div class="card-block">
        <%= render partial: 'shared/time_spent' %>
